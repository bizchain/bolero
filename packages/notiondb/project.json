{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "packages/notiondb/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/notiondb",
        "main": "packages/notiondb/src/index.ts",
        "tsConfig": "packages/notiondb/tsconfig.lib.json",
        "assets": ["packages/notiondb/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/notiondb/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/notiondb"],
      "options": {
        "jestConfig": "packages/notiondb/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
